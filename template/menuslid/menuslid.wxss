@keyframes mymove {
  from {
    transform: rotate(0deg) scale(1) translate(0px, 0%);
  }
  to {
    transform: rotate(0deg) scale(1) translate(200px, 0%);
  }
}
@-webkit-keyframes mymove {
  /* Safari and Chrome */
  from {
    -webkit-transform: rotate(0deg) scale(1) translate(0px, 0%);
  }
  to {
    -webkit-transform: rotate(0deg) scale(1) translate(200px, 0%);
  }
}
.c-state1 {
  transform: rotate(0deg) scale(1) translate(200px, 0%);
  -webkit-transform: rotate(0deg) scale(1) translate(200px, 0%);
  animation: mymove .5s;
  -webkit-animation: mymove .5s;
}
.page-bottom {
  width: 200px;
  position: fixed;
  top: 0;
  left: -200px;
  z-index: -1;
  height: 100%;
  background: #E8E8E8;
}
.page-bottom .wc {
  text-align: center;
  margin-left: 10px;
  margin-right: 10px;
  line-height: 100rpx;
  border-bottom: 1px solid #ddd;
}
.page-bottom .wc:first-child {
  margin-top: 150rpx;
}
.c-state2 {
  position: absolute;
  width: 100%;
  height: 100%;
  border: 1px solid;
  background: #000;
  opacity: .5;
  z-index: 1000;
}
.head {
  height: 100rpx;
}
.head .ic {
  width: 50rpx;
  height: 50rpx;
  margin: 20rpx;
}
